{"remainingRequest":"/Users/marcguldner/GitHub/emc-data-server/frontend/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/marcguldner/GitHub/emc-data-server/frontend/src/components/maps.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/marcguldner/GitHub/emc-data-server/frontend/src/components/maps.vue","mtime":1598867441172},{"path":"/Users/marcguldner/GitHub/emc-data-server/frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/marcguldner/GitHub/emc-data-server/frontend/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/marcguldner/GitHub/emc-data-server/frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/marcguldner/GitHub/emc-data-server/frontend/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["maps.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"maps.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div>\n    <div v-if=\"Rtwdocument.lat && Rtwdocument.long\">\n      <l-map :zoom=\"zoom\" :center=\"center\" v-bind:style=\"styleObject\">\n        <l-tile-layer :url=\"url\" :attribution=\"attribution\"></l-tile-layer>\n        <l-marker :lat-lng=\"markerUMM\">\n          <l-tooltip :options=\"{ permanent: true, interactive: true }\">\n            UMM\n          </l-tooltip>\n        </l-marker>\n        <l-marker :lat-lng=\"markerPatient\">\n          <l-tooltip :options=\"{ permanent: true, interactive: true }\">\n            Patient\n          </l-tooltip>\n        </l-marker>\n      </l-map>\n    </div>\n    <div v-else>\n      <div v-if=\"typeof Rtwdocument.eta === 'string'\">\n        {{ Rtwdocument.eta }} - Komponente kann aktuell nicht geladen werden.\n      </div>\n      <div v-else>\n        {{ stateMessage }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n//Leaflet components\nimport L from \"leaflet\";\nimport { LMap, LTileLayer, LMarker, LTooltip } from \"vue2-leaflet\";\nimport { Icon } from \"leaflet\";\ndelete Icon.Default.prototype._getIconUrl;\nIcon.Default.mergeOptions({\n  iconRetinaUrl: require(\"leaflet/dist/images/marker-icon-2x.png\"),\n  iconUrl: require(\"leaflet/dist/images/marker-icon.png\"),\n  shadowUrl: require(\"leaflet/dist/images/marker-shadow.png\")\n});\nexport default {\n  data() {\n    return {\n      zoom: 15,\n      center: L.latLng(this.Rtwdocument.lat, this.Rtwdocument.long),\n      url: \"http://{s}.tile.osm.org/{z}/{x}/{y}.png\",\n      attribution:\n        '&copy; <a href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors',\n      markerUMM: L.latLng(49.492427, 8.487255),\n      markerPatient: L.latLng(this.Rtwdocument.lat, this.Rtwdocument.long),\n      stateMessage: \"Karte wird geladen...\"\n    };\n  },\n  components: {\n    LMap,\n    LTileLayer,\n    LMarker,\n    LTooltip\n  },\n  props: {\n    Rtwdocument: Object,\n    selectedElements: Number\n  },\n  watch: {\n    Rtwdocument: {\n      handler() {\n        this.adjustPosition(this.Rtwdocument.lat, this.Rtwdocument.long);\n        if (typeof this.Rtwdocument.eta === \"string\") {\n          this.stateMessage = this.Rtwdocument.eta;\n        }\n      },\n      deep: true\n    }\n  },\n  methods: {\n    adjustPosition: function(lat, long) {\n      this.markerPatient = L.latLng(lat, long);\n      this.center = L.latLng(lat, long);\n    }\n  },\n  computed: {\n    styleObject: function() {\n      if (this.selectedElements === 1) {\n        return { height: \"65vh\", width: \"62vw\" };\n      } else {\n        return { height: \"25vh\", width: \"62vw\" };\n      }\n    }\n  }\n};\n</script>\n"]}]}